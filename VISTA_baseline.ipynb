#packages/dependencies
import subprocess, sys

packages = [
    'yfinance',
    'mplfinance',
    'pandas',
    'numpy',
    'torch',
    'torchvision',
    'timm', #pretrained CNN backbones (EfficientNet, ViT, etc.)
    'scikit-learn',
    'lightgbm',
    'shap',
    'plotly',
    'ta', #technical analysis library
    'Pillow',
    'tqdm',
    'matplotlib',
    'seaborn',
    'vectorbt', #backtesting
    'ipywidgets', #interactive notebook widgets
]

for pkg in packages:
    subprocess.check_call([sys.executable, '-m', 'pip', 'install', pkg, '-q'])

print('All packages installed! Yay Anna!')
